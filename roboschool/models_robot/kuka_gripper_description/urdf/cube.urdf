<?xml version="1.0"?>
<robot name="cube">

  <link name="cube">
    <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.06 0.06 0.06"/>
      </geometry>
      <material name="solid_color">
        <color rgba="0.6 0.3 0 1"/>
      </material>
    </visual>

    <collision>
      <geometry>
        <box size="0.06 0.06 0.06"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.6"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.001"/>  <!-- I = m * l^2 / 6 -->
    </inertial>
  </link>

</robot>

